@model IEnumerable<project_management_for_ISOGES.Entities.CobroEnt>
@{
    ViewBag.Title = "Cobros";
}
<div class="body-wrapper">
    <div class="row">
        <div class="col-12">

            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Fecha Limite</th>
                        <th>Tipo de cobro</th>
                        <th>Estado</th>
                        <th>Monto</th>
                    </tr>
                <thead>
                <tbody>
                    @foreach (var item in Model)
                    {

                        <tr>
                            <td>@item.FechaSola</td>
                            <td>@item.NombreTipoCobro</td>
                            <td>@item.NombreEstado</td>
                            <td>@item.Monto</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
    <div class="row">
        <div class="col-8">
            <div style="text-align:center" class="" role="alert">
                @ViewBag.MsjPantalla
            </div>
        </div>
    </div>
</div>
